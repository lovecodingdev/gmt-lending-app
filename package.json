        "@solana/web3.js": "^1.36.0",
        "react": "^17.0.0",
        "react-dom": "^17.0.0",
        "react-router-dom": "^6.3.0",
        "@pythnetwork/client": "^2.5.1",
        "@solana/buffer-layout": "^3.0.0",
        "@solana/spl-token": "^0.1.8",
        "axios": "^0.24.0",
        "bignumber.js": "^9.0.2",
